<Application x:Class="Desk.Aesthetics.PinnedImages.Presentation.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Desk.Aesthetics.PinnedImages.Presentation"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="StylesDictionaries/Images.xaml"/>
                <ResourceDictionary Source="StylesDictionaries/Types.xaml"/>
                <ResourceDictionary Source="StylesDictionaries/Drawables.xaml"/>
                <ResourceDictionary Source="StylesDictionaries/Buttons.xaml"/>
                <ResourceDictionary Source="StylesDictionaries/Windows.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
